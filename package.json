{
	"private": true,
	"scripts": {
		"dev": "npm run jas && vite",
		"build": "npm run jas && vite build",
		"jas": "php artisan ziggy:generate && php artisan zora:generate"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"header-max-length": [
				2,
				"always",
				120
			],
			"type-enum": [
				2,
				"always",
				[
					"breaking",
					"feat",
					"fix",
					"refactor",
					"docs",
					"chore"
				]
			]
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^16.0.1",
		"@commitlint/config-conventional": "^16.0.0",
		"@inertiajs/inertia": "^0.10.0",
		"@inertiajs/inertia-vue3": "^0.5.1",
		"@inertiajs/progress": "^0.2.6",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/typography": "^0.5.0",
		"@vitejs/plugin-vue": "^2.0.1",
		"@vue/compiler-sfc": "^3.0.5",
		"axios": "^0.21",
		"dayjs": "^1.10.7",
		"dotenv": "^10.0.0",
		"husky": "^7.0.4",
		"larasocket-js": "^1.0.16",
		"laravel-echo": "^1.11.3",
		"lodash": "^4.17.19",
		"mitt": "^3.0.0",
		"postcss": "^8.1.14",
		"postcss-import": "^12.0.1",
		"semantic-release": "^18.0.1",
		"tailwindcss": "^3.0.0",
		"unplugin-auto-import": "^0.5.5",
		"unplugin-vue-components": "^0.17.11",
		"vite": "^2.7.10",
		"vue": "^3.0.5",
		"vue-loader": "^16.1.2",
		"vuex": "^4.0.2"
	}
}
